{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "buildCommand": "cargo build --release -p specter-cli"
  },
  "deploy": {
    "startCommand": "find target/release -name \"specter\" -type f -exec {} serve --port $PORT \\;",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 300
  }
}