# ─── Network ─────────────────────────────────────────────────────────────────
# Use Sepolia testnet (true) or mainnet (false). When true, ETH_RPC_URL defaults to Sepolia.
USE_TESTNET=false

# ─── Ethereum RPC ───────────────────────────────────────────────────────────
# Required for ENS resolution. If unset: Sepolia when USE_TESTNET=true, mainnet when false.
# Mainnet:
ETH_RPC_URL=https://ethereum.publicnode.com
# Sepolia (when USE_TESTNET=true):
# ETH_RPC_URL=https://ethereum-sepolia-rpc.publicnode.com

# ─── Pinata IPFS ────────────────────────────────────────────────────────────
# Dedicated gateway (required for IPFS retrieves; no other gateways used)
PINATA_GATEWAY_URL=beige-hollow-ermine-440.mypinata.cloud
PINATA_GATEWAY_TOKEN=your_gateway_token_from_pinata_dashboard

# JWT for meta-address uploads (v3 API)
PINATA_JWT=your_pinata_jwt_token

# ─── Sui RPC ──────────────────────────────────────────────────────────────
# Required for SuiNS resolution. If unset: testnet when USE_TESTNET=true, mainnet when false.
SUI_RPC_URL=https://fullnode.mainnet.sui.io:443

# ─── Optional ──────────────────────────────────────────────────────────────
ENABLE_CACHE=true
