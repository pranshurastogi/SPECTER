VITE_API_BASE_URL=http://localhost:3001

# IPFS Gateway Configuration
VITE_IPFS_GATEWAY=https://ipfs.io
VITE_IPFS_GATEWAY_TIMEOUT_MS=10000

# ENS Resolution Configuration
