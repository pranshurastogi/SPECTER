{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "buildCommand": "cd specter && cargo build --release -p specter-cli"
  },
  "deploy": {
    "startCommand": "cd specter && ./target/release/specter serve --port $PORT",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 30
  }
}
